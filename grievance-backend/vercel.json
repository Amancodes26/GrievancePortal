{
    "version": 2,
    "builds": [
        { 
            "src": "src/index.ts", 
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "src/index.ts" }],
    "functions": {
        "src/index.ts": {
            "maxDuration": 50
        }
    },
    "env": {
        "VERCEL": "true"
    }
}